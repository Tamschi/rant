<$a = (a; b; c; d; e; f; g; h)>

[assert-eq: <a/0..>; (a; b; c; d; e; f; g; h)]
[assert-eq: <a/ 0 .. >; (a; b; c; d; e; f; g; h)]
[assert-eq: <a/-8..>; (a; b; c; d; e; f; g; h)]
[assert-eq: <a/ -8 .. >; (a; b; c; d; e; f; g; h)]

[assert-eq: <a/1..>; (b; c; d; e; f; g; h)]
[assert-eq: <a/ 1 .. >; (b; c; d; e; f; g; h)]

[assert-eq: <a/-7..>; (b; c; d; e; f; g; h)]
[assert-eq: <a/ -7 .. >; (b; c; d; e; f; g; h)]

[assert-eq: <a/-1..>; (h;)]
[assert-eq: <a/ -1 .. >; (h;)]