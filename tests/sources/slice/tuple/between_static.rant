<$a = (a; b; c; d; e; f; g; h)>

[assert-eq: <a/0:8>; (a; b; c; d; e; f; g; h)]
[assert-eq: <a/8:0>; (a; b; c; d; e; f; g; h)]

[assert-eq: <a/0:4>; (a; b; c; d)]
[assert-eq: <a/4:0>; (a; b; c; d)]

[assert-eq: <a/4:8>; (e; f; g; h)]
[assert-eq: <a/8:4>; (e; f; g; h)]

[assert-eq: <a/1:-1>; (b; c; d; e; f; g)]
[assert-eq: <a/-1:1>; (b; c; d; e; f; g)]

[assert-eq: <a/-4:-1>; (e; f; g)]
[assert-eq: <a/-1:-4>; (e; f; g)]

[assert-eq: <a/0:0>; ()]